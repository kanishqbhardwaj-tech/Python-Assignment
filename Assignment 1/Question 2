def classify_files(mapping_str, filenames):

    extension_map = {}
    pairs = mapping_str.split(';')
    for pair in pairs:
        if ',' in pair:
            ext, file_type = pair.split(',')
            extension_map[ext.strip()] = file_type.strip()

    results = []

 
    for name in filenames:
        if '.' in name:
        
            ext = name.split('.')[-1]
       
            file_type = extension_map.get(ext, "unknown")
        else:
            
            file_type = "unknown"
            
        results.append(f"{name} -> {file_type}")
    
    return results


mapping = "xls,spreadsheet;xlsx,spreadsheet;jpg,image;html,webpage"
files = ["abc.html", "xyz.xls", "text.csv", "123"]

output = classify_files(mapping, files)
for line in output:
    print(line)